(seq
	#unit_test (direct_assign_to_entities (assoc unit_test (load "unit_test.amlg")))
	(call (load "unit_test_howso.amlg") (assoc name "ut_h_continuous_distribution.amlg"))

	(call_entity "howso" "create_trainee" (assoc trainee "test" ))
	(assign_to_entities "howso" (assoc trainee "test"))


	(declare (assoc
		model_data
			(list
				(list "x" "y")
				(list 4 10)
				(list 4 20)
				(list 4 30)
				(list 4 40)
				(list 4 50)
				(list 4 60)
				(list 4 70)
				(list 4 80)
			)

	))
	(declare (assoc
		data (tail model_data)
		features (first model_data)
		a10 0
		a20 0
		a30 0
		a40 0
		a50 0
		a60 0
		a70 0
		a80 0
		other 0
		n 1000
	))

	(call_entity "howso" "set_internal_parameters" (assoc
		hyperparameter_map (assoc ".targetless" (assoc "robust" (assoc ".none" (assoc "k" 2 "p" 2 "dt" -1))))
	))

	(call_entity "howso" "train" (assoc
		input_cases data
		features features
		session "session1"
	))

	(range (lambda
		(seq
			(assign (assoc
				output
					(first (get
						(call_entity "howso" "react" (assoc
							action_features (list  "y")
							context_features (list "x")
							context_values (list 2)
							desired_conviction .infinity
						))
						(list "payload" "action_values")
					))
			))


			(if (= output 10)
				(accum (assoc a10 1))

				(= output 20)
				(accum (assoc a20 1))

				(= output 30)
				(accum (assoc a30 1))

				(= output 40)
				(accum (assoc a40 1))

				(= output 50)
				(accum (assoc a50 1))

				(= output 60)
				(accum (assoc a60 1))

				(= output 70)
				(accum (assoc a70 1))

				(= output 80)
				(accum (assoc a80 1))

				(accum (assoc other 1))
			)

		 ))
		1 n 1
	)

	(print "10 " a10 " ")
	(call assert_approximate (assoc obs a10 exp 125 thresh 45))

	(print "20 " a20 " ")
	(call assert_approximate (assoc obs a20 exp 125 thresh 45))

	(print "30 " a30 " ")
	(call assert_approximate (assoc obs a30 exp 125 thresh 45))

	(print "40 " a40 " ")
	(call assert_approximate (assoc obs a40 exp 125 thresh 45))

	(print "50 " a50 " ")
	(call assert_approximate (assoc obs a50 exp 125 thresh 45))

	(print "60 " a60 " ")
	(call assert_approximate (assoc obs a60 exp 125 thresh 45))

	(print "70 " a70 " ")
	(call assert_approximate (assoc obs a70 exp 125 thresh 45))

	(print "80 " a80 " ")
	(call assert_approximate (assoc obs a80 exp 125 thresh 45))

	(print "other " other " ")
	(call assert_same (assoc obs other exp 0))


	(call exit_if_failures (assoc msg unit_test_name ))
)


