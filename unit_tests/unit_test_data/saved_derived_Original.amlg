														(get
															best_params
															(floor
																(/
																	(size
																		best_params
																	)
																	2
																)
															)
														)
													)
											)
										)
										(assign
											(assoc
												output_map (assoc
														dwe (get
																best
																"dwe"
															)
														k (get
																best
																"k"
															)
														gridSearchError (get
																best
																"distances"
															)
														p (get
																best
																"p"
															)
													)
											)
										)

