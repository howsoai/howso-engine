																)
															)
															(values
																accumulated_results_map
															)
														)
													)
												best_p (get
														accumulated_results_map
														(list
															best_params_key
															"p"
														)
													)
											)
										)

										;sort the best remaining params that have all the same 'best' p and accuracy distance by their K value
										(assign
											(assoc
												best_params (sort
														(lambda
															(<
																(get
																	(current_value
																		1
																	)
																	"k"
																)
																(get
																	(current_value)
																	"k"
																)
															)
														)

														;filter out all params that did not have a matching p
														(filter
															(lambda
																(=
																	(get
																		(current_value)
																		"p"
