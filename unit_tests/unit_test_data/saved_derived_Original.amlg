																			(>
																				(current_value)
																				1e-13
																			)
																			(/
																				1
																				(pow
																					(current_value)
																					(get
																						output_map
																						"p"
																					)
																				)
																			)

																			;else set it to 1 because there is no residual
																			1
																		)
