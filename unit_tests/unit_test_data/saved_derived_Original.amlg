														(lambda
															(<
																(get
																	(current_value
																		1
																	)
																	"p"
																)
																(get
																	(current_value)
																	"p"
																)
															)
														)
														(filter
															(lambda
																(=
																	(get
																		(current_value)
																		"distances"
																	)
