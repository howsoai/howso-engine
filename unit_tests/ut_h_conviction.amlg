			))
	))
	(print "Using a condition: ")
	(call assert_not_null (assoc obs (get conviction_w_condition [1 "payload" "familiarity_conviction_addition"])))

	(declare (assoc
		conviction_w_indices
			(call_entity "howso" "react_group" (assoc
				case_indices [[["none" 0] ["none" 1] ["none" 2] ["none" 3]]]
				features iris_features
				familiarity_conviction_addition .true
			))
	))
	(print "Using case_indices: ")
	(call assert_not_null (assoc obs (get conviction_w_indices [1 "payload" "familiarity_conviction_addition"])))

	(call exit_if_failures (assoc msg unit_test_name ))
