	))
	(call assert_true (assoc
		obs (contains_value (get result (list 1 "details" 0)) ".*initial_values.*")
	))

	(call exit_if_failures (assoc msg "Invalid datetime string errors."))

	(call exit_if_failures (assoc msg unit_test_name))
)