						"influential_cases" .true
						"influential_cases_familiarity_convictions" .true
						"boundary_cases" .true
						"similarity_conviction" .true
						"outlying_feature_values" .true
					)
			))
	))
	(call keep_result_payload)

	(declare (assoc
		result2
			(get
