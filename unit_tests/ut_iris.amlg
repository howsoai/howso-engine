				context_values (list 5.4 2.3 1.5 0.1)
				action_features action_labels
				details (assoc "influential_cases" .true "relevant_values" .true)
			))
	))
	(call keep_result_payload)

	(print "Relevant values are returned (and each feature has 5 values): ")
	(call assert_true (assoc
		obs (apply "=" (append [5] (values (map (lambda (size (current_value))) (get result "relevant_values")))))
	))
	(print "And they are non-null: ")
	(call assert_true (assoc
		obs
			(not (contains_value
				(apply "append" (values (get result "relevant_values")))
				(null)
			))
	))

